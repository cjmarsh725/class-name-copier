{
  "name": "classname-copier",
  "version": "1.0.0",
  "description": "A VS Code extension that makes it easier to copy JSX classNames into CSS files",
  "author": "Chris Marsh",
  "main": "./extension.js",
  "repository": "https://github.com/cjmarsh725/classname-copier.git",
  "homepage": "https://github.com/cjmarsh725/classname-copier#readme",
  "engines": {
    "vscode": "^1.32.0"
  },
  "activationEvents": [
    "onCommand:extension.classNameCopier"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.classNameCopier",
        "title": "className Copier"
      }
    ],
    "keybindings":[
      {
        "command": "extension.classNameCopier",
        "key": "Ctrl+Shift+Alt+C"
      }
    ]
  },
  "devDependencies": {
    "vscode": "^1.1.35"
  }
}
